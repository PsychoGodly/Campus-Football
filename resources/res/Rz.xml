<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="25dp"/>
            <solid android:color="@color/applovin_sdk_switchTrackDisabledColor"/>
            <size android:height="25dp" android:width="50dp"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="25dp"/>
            <solid android:color="@color/applovin_sdk_brand_color"/>
            <size android:height="25dp" android:width="50dp"/>
        </shape>
    </item>
</selector>
